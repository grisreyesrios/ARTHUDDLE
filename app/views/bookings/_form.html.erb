<%= simple_form_for ([@workshop, @booking]) do |b| %>
<div class="container">
  <div class="col-md-4 mb-3">
  <%= b.input :theme , class: 'form-control' %>
  </div>
</div>
<div class="form-group row">
  <div class="col-md-4 mb-3">
  <%= b.input :date, input_html: { class: "datepicker", value: Time.now.strftime('%d-%m-%Y') } %>
</div>
</div>
    <%= b.button :submit, class: 'btn btn-primary' %>
<% end %>
